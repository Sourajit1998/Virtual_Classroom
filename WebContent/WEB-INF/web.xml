<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>Student_login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>StudentLogin</servlet-name>
    <servlet-class>controller.StudentLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentLogin</servlet-name>
    <url-pattern>/stud_log</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StudentRegister</servlet-name>
    <servlet-class>controller.StudentRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentRegister</servlet-name>
    <url-pattern>/regis</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StudentProfile</servlet-name>
    <servlet-class>controller.StudentProfile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentProfile</servlet-name>
    <url-pattern>/stud_pro</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FacultyLogin</servlet-name>
    <servlet-class>controller.FacultyLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FacultyLogin</servlet-name>
    <url-pattern>/fac_log</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FacultyProfile</servlet-name>
    <servlet-class>controller.FacultyProfile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FacultyProfile</servlet-name>
    <url-pattern>/fac_pro</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FileUpload</servlet-name>
    <servlet-class>controller.FileUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DisplayUploadedFiles</servlet-name>
    <servlet-class>controller.UploadedFiles</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayUploadedFiles</servlet-name>
    <url-pattern>/disp_files</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>FileDownload</servlet-name>
    <servlet-class>controller.FileDownload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileDownload</servlet-name>
    <url-pattern>/download</url-pattern>
  </servlet-mapping>
</web-app>